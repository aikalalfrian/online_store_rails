# Ignore bundler config.
.bundle

# Ignore all environment files.
.env*
# Ignore database configuration file
/config/database.yml

# Ignore all logfiles and tempfiles.
log/*
tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pidfiles, but keep the directory.
tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore storage (uploaded files in development and any SQLite databases).
storage/*
!/storage/.keep
/tmp/storage/*
!/tmp/storage/
!/tmp/storage/.keep

# Ignore master key for decrypting credentials and more.
config/master.key

# Ignore test files and coverage reports
/test/tmp/*
/coverage/*
/spec/tmp/*

# Ignore any secret files
/config/initializers/secret_token.rb

# Ignore any files generated by IDEs or editors
*.swp
*.swo
*.DS_Store
*.idea/
*.vscode/
*.project
*.classpath

# Ignore any backup files
*~
*.bak
*.orig

# Ignore yarn and npm dependencies
node_modules/
yarn-error.log
package-lock.json
